<Window x:Class="assisgment2analysis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox Name="StartParishComboBox" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox Name="EndParishComboBox" HorizontalAlignment="Left" Margin="150,10,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Name="AStarButton" Content="A* Search" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="120" Click="AStarButton_Click"/>
        <Button Name="BestFirstButton" Content="Best First Search" HorizontalAlignment="Left" Margin="150,50,0,0" VerticalAlignment="Top" Width="120" Click="BestFirstButton_Click"/>
        <Button Name="GreedyButton" Content="Greedy Search" HorizontalAlignment="Left" Margin="290,50,0,0" VerticalAlignment="Top" Width="120" Click="GreedyButton_Click"/>
        <Canvas Name="GraphCanvas" Margin="10,100,10,10"/>
    </Grid>
</Window>